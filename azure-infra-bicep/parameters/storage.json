{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageConfig": {
      "value": {
        "storageAccountName": "devstorageacct01",
        "location": "[parameters('location')]",
        "sku": {
          "name": "Standard_LRS",
          "tier": "Standard"
        },
        "kind": "StorageV2",
        "accessTier": "Hot",
        "tags": {
          "env": "[parameters('env')]",
          "project": "aftra"
        },
        "accessKeySecretName": "[parameters('accessKeySecretName')]",
        "allowedIpAddresses": [],
        "factoryResourceIds": [],
        "subnetIds": [],
        "blobContainers": [
          "aks-logs",
          "coda-dev",
          "coda-documentuploads",
          "coda-fileattachments",
          "codaqa"
        ],
        "fileShares": [
          "downloaded-claims",
          "oc-payment-files"
        ]
      }
    }
  }
}
