{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageConfig": {
      "value": {
        "storageAccountName": "devstorageacct",
        "location": "eastus",
        "sku": { "name": "Standard_LRS" },
        "kind": "StorageV2",
        "accessTier": "Hot",
        "vnetName": "myVnet",
        "subnetNames": ["subnet1", "subnet2"],
        "factoryNames": ["DataFactory1"],
        "allowedIpAddresses": ["52.168.1.1", "52.168.1.2"],
        "blobContainers": ["container1", "container2"],
        "fileShares": ["fileshare1", "fileshare2"]
      }
    },
    "tagSuffix": {
      "value": "dev"
    }
  }
}
